        public static void mostrarPropiedadesTrabajador(int edad , String nombre, String telefono,float salario,Trabajador trabajador){
                    trabajador.edad=edad;
                    trabajador.nombre=nombre;
                    trabajador.telefono=telefono;
                    trabajador.setSalasrio(salario);
                    System.out.println("\n*******************************************");
