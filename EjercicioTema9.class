    public static void mostrarPropiedadesCliete(int edad , String nombre, String telefono,String credito,Cliente cliente){
                    cliente.edad=edad;
                    cliente.nombre=nombre;
                    cliente.telefono=telefono;
                    cliente.setCredito(credito);
                        System.out.println("*******************************************");
